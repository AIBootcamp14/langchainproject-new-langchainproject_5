# ==================== API 키 설정 ==================== #
# OpenAI API
OPENAI_API_KEY=sk-...

# Solar API (Upstage)
SOLAR_API_KEY=up-...

# Tavily Search API
TAVILY_API_KEY=tvly-...


# ==================== PostgreSQL 설정 ==================== #
# PostgreSQL 연결 정보
POSTGRES_USER=your_username
POSTGRES_PASSWORD=your_password
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=papers


# ==================== 개발 환경 설정 ==================== #
# 환경 모드 (development, production)
ENVIRONMENT=development

# 로그 레벨 (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO


# ==================== LLM 모델 설정 ==================== #
# OpenAI 모델 설정
LLM_MODEL=gpt-5
LLM_TEMPERATURE=0.7
LLM_MAX_TOKENS=2000

# Solar 모델 설정
SOLAR_MODEL=solar-pro2
SOLAR_TEMPERATURE=0.7
SOLAR_MAX_TOKENS=2000


# ==================== Embeddings 설정 ==================== #
# 임베딩 모델 설정
EMBEDDING_MODEL=text-embedding-3-small
EMBEDDING_DIMENSION=1536
